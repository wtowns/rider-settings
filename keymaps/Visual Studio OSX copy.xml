<keymap version="1" name="Visual Studio OSX copy" parent="Visual Studio OSX">
  <action id="Back">
    <keyboard-shortcut first-keystroke="meta minus" />
    <keyboard-shortcut first-keystroke="meta subtract" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="meta open_bracket" />
  </action>
  <action id="ChangeSplitOrientation">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="space" />
  </action>
  <action id="EditorCodeBlockEnd">
    <keyboard-shortcut first-keystroke="meta asterisk" />
  </action>
  <action id="EditorCodeBlockStart" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="shift meta minus" />
    <keyboard-shortcut first-keystroke="shift meta subtract" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="meta close_bracket" />
  </action>
  <action id="MoveTabDown">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="shift j" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="shift l" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="shift k" />
  </action>
</keymap>